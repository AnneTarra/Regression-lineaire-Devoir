Fonction<-function(data){
matrix_coef<- summary(lm(y ~ .,data=data))
matrix_estimates<- matrix_coef$coefficients[,1]
matrix_tvalue<- matrix_coef$coefficients[,3]
return(data.frame(matrix_estimates,matrix_tvalue))
}
